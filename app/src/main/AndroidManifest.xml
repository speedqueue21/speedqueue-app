<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        package="com.weebkun.skipdq">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
            android:name=".SkipDQ"
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/Theme.Skipdq">
        <activity android:name=".OrdersActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".RegisterSchoolActivity" />
        <activity android:name=".HelpActivity" />
        <activity android:name=".FavouritesActivity" />
        <activity android:name=".UserProfileActivity" />
        <activity android:name=".SettingsActivity" />
        <activity android:name=".CurrentOrdersActivity" />
        <activity android:name=".CheckoutActivity" />
        <activity android:name=".ItemActivity" />
        <activity android:name=".MenuActivity" />
        <activity android:name=".StallSelectActivity" />
        <activity android:name=".FoodCourtSelectActivity" />
        <activity android:name=".SchoolSelectActivity" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>